<Window x:Class="WPF_WorldMap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_WorldMap"
        mc:Ignorable="d"
        Title="XAML World Map High Resolution W:5350px H:3330px"  Icon="/material-io-baseline_explore_black_18dp.png">
    <Grid Background="#f6f5f3">
        <Grid.RowDefinitions>
            <RowDefinition Height="100"/>
            <RowDefinition Height="104*"/>
          
        </Grid.RowDefinitions>
        <Border Background="#FF555150">
            <TextBlock Text="XAML World Map High Resolution W:5350px H:3330px" FontSize="30" HorizontalAlignment="Center" VerticalAlignment="Center" Foreground="#f6f5f3"/>
        </Border>
       

            <Viewbox x:Name="MAPViewBox"  Stretch="Uniform" Grid.Row="1">
            <local:WorldMapUserControl x:Name="WorldMap" >
                
            </local:WorldMapUserControl>
        </Viewbox>
     
    </Grid>
</Window>
